(define (fixed-point f first-guess)
    (define (close-enough? a b)
        (< (abs (- a b)) 0.000001))
	(define (try guess)
        (let ((next (f guess)))
			(cond ((close-enough? guess next) next)
                  (else (display next)
				        (newline)
					    (try next)))))
    (try first-guess))
